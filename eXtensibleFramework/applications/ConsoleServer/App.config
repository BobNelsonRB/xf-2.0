<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="eXtensoft.application" type="XF.Common.eXtensibleFrameworkSection, XF.Common.Contracts, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
    <sectionGroup name="eXtensoft.database" type="XF.Common.eXtensibleStrategySectionGroup, XF.Common.Contracts, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
      <section name="appOne" type="XF.Common.eXtensibleStrategySection, XF.Common.Contracts, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
    </sectionGroup>
    <sectionGroup name="enyim.com">
      <section name="memcached" type="Enyim.Caching.Configuration.MemcachedClientSection, Enyim.Caching" />
    </sectionGroup>
    <section name="cacheProvider" type="MemcachedProviders.Cache.CacheProviderSection, MemcachedProviders" allowDefinition="Everywhere" restartOnExternalChanges="true" />

  </configSections>

  <eXtensoft.application zone="Development" appContext="server" instanceId="demo.console.server" 
                         dataFolderpath="data" isAsync="false" userIdParamName="UserIdentity" 
                         captureMetrics="true" loggingKey="x" bigDataUrl="http://localhost:49408/" serviceToken="43b1e64e-b990-4fc5-a727-bf124b274ba8">
    <loggingSettings>
      <add key="a" loggingStrategy="WindowsEventlog" publishSeverity="Verbose" />
      <add key="b" loggingStrategy="CommonServices" publishSeverity="Verbose" />
      <add key="c" loggingStrategy="XFTool" publishSeverity="Critical" />
      <add key="d" loggingStrategy="Plugin" publishSeverity="Verbose" />
      <add key="o" loggingStrategy="Output" publishSeverity="Verbose" />
      <add key="x" loggingStrategy="XFTool" publishSeverity="Verbose" inform="true"/>
    </loggingSettings>
  </eXtensoft.application>

  <eXtensoft.database>
    <appOne appContext="my.app" resolvesTo="demo-key">
      <strategies>
        <add name="Widget" strategyValue="Company.Technology" resolvesTo="arc-demo" />
        <add name="Broomstick" strategyValue="Company.Technology.Person" resolvesTo="arc-demo" />
        <add name="big-data" strategyValue="XF.Common.MongoDb" resolvesTo="bigdata.mongo" />
        <!--<add name="Patron" strategyValue="Company.Technology.Patron" resolvesTo="zack" />-->
      </strategies>
    </appOne>
  </eXtensoft.database>

  <appSettings file="">
    <clear />
    <!--<add key="eXtensoft.app.context.key" value="demo"/>
    <add key="eXtensoft.app.zone.key" value="development"/>
    <add key="eXtensoft.app.logging.strategy" value="console"/>
    <add key="eXtensoft.app.logging.severity" value="verbose"/>
    <add key="eXtensoft.app.connectionstring.key" value="demo"/>-->
  </appSettings>

  <connectionStrings>
    <add name="arc-demo" connectionString="Data Source=(local);Initial Catalog=Demo;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="bigdata.mongo" connectionString="Data Source=mongodb://localhost;Initial Catalog=BigData;" providerName="MongoDb.Driver" />
    <add name="my.sql" connectionString="server=localhost;user id=root; password=; database=mysql; pooling=false" providerName="MySql.Data" />
    <add name="demo-key1" connectionString="Data Source=RBDEV02;Initial Catalog=OneClick_Replica;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="demo-key2" connectionString="Data Source=RBDEV02;Initial Catalog=OneClick_Replica;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="demo" connectionString="Data Source=RBDEV02;Initial Catalog=OneClick_Replica;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="architecture.demo" connectionString="Data Source=rbdev02;Initial Catalog=OneClick_Replica;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="bigArrays" maxReceivedMessageSize="2147483647">
          <readerQuotas maxDepth="64"
												maxStringContentLength="2147483647"
												maxArrayLength="2147483647"
												maxBytesPerRead="4096"
												maxNameTableCharCount="16384"/>
        </binding>
      </basicHttpBinding>
      <wsHttpBinding>
        <binding name="wsBigArrays" maxReceivedMessageSize="2147483647">
          <readerQuotas maxDepth="64" maxStringContentLength="2147483647"
				    maxArrayLength="2147483647" maxBytesPerRead="4096" maxNameTableCharCount="16384"/>
        </binding>
      </wsHttpBinding>
    </bindings>
    <services>
      <!--<service name="XF.Common.Wcf.DataPacketService"
							 behaviorConfiguration="serviceBehavior">
        <host>
          <baseAddresses>
            <add baseAddress = "http://localhost:6000" />
          </baseAddresses>
        </host>
        <endpoint address =""
									bindingConfiguration="wsBigArrays"
									binding="wsHttpBinding"
									contract="XF.Common.Wcf.IDataPacketService">
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
      </service>-->
      <service name="XF.Common.Wcf.GenericService"
							 behaviorConfiguration="serviceBehavior">
        <host>
          <baseAddresses>
            <add baseAddress = "http://localhost:6000" />
          </baseAddresses>
        </host>
        <endpoint address ="/generic"
									bindingConfiguration="wsBigArrays"
									binding="wsHttpBinding"
									contract="XF.Common.Wcf.IDataPacketService">
        </endpoint>
          <endpoint address ="/rpc"
									bindingConfiguration="wsBigArrays"
									binding="wsHttpBinding"
									contract="XF.Common.Wcf.IRpcDataPacketService">
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="serviceBehavior">
          <serviceMetadata httpGetEnabled="True"/>
          <serviceDebug includeExceptionDetailInFaults="True" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>


  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="model"/>
    </assemblyBinding>
  </runtime>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
    </startup>
</configuration>